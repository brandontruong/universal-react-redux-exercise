    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "express": "^4.13.4",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "enzyme": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
